				{
					"name": "Creating with all valid fields",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201 Created\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Response has correct imsx_codeMajor value\", function () {",
									"    const res = pm.response.json();",
									"    pm.expect(res.imsx_codeMajor).to.eql(\"created\");",
									"});",
									"",
									"pm.test(\"Supplied sourcedId matches request value\", function () {",
									"    const res = pm.response.json();",
									"    pm.expect(res.sourcedIdPairs.suppliedSourcedId).to.eql(JSON.parse(pm.request.body.raw).sourcedId);",
									"});",
									"",
									"pm.test(\"Allocated sourcedId is a valid UUID\", function () {",
									"    const res = pm.response.json();",
									"    pm.expect(res.sourcedIdPairs.allocatedSourcedId).to.match(/^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$/i);",
									"});"
